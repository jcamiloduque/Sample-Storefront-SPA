BAA+C,IAA/C,EAA+C,IAA/C,EAA+C,CAA3CE,aAAsCC,sBAAtC,EAAsC;AAAzBC,MAAE,EAAC;AAAsB,GAAtC,EAAmB;sBAAC;AAAA,aAAI,YAAJ;AAAA,MAAD;;;;AAAA,GAAnB,CAA2C,CAA/C,CAEC,EADDC,UACC,CAHL,CAwCE,EApCFL,oBAkCM,KAlCN,cAkCM,CAjCcM,sCAAhBL,oBA0BWM,SA1BX,EA0BW;AAAAC;AAAA,GA1BX,EA0BW,CAzBPR,oBASM,KATN,cASM,CARFA,oBAOQ,OAPR,EAOQ,IAPR,EAOQ,CANJS,UAMI,EADJP,aAA+BQ,oBAA/B,EAA+B;AAAnBC,YAAM,EAAEL;AAAW,GAA/B,EAA2B,IAA3B,EAA2B;AAAA;AAA3B,IAA2B,YAA3B,CACI,CAPR,CAQE,CATN,CAyBO,EAfPN,oBAcM,KAdN,cAcM,CAbFA,oBAEM,KAFN,cAEM,CADFA,oBAA0E,QAA1E,EAA0E;AAAlED,SAAK,EAAC,MAA4D;AAApDa,WAAK;AAAA,aAAEC,aAAQC,IAAR,CAAY,GAAZ,CAAF;AAAA;AAA+C,GAA1E,EAAgD,mBAAhD,CACE,CAFN,CAaE,EAVFC,WAUE,EANFf,oBAEM,KAFN,eAEMgB,iBADAV,cACA,CAFN,EACW;AAAA;AADX,GAME,EAHFW,WAGE,CAdN,CAeO,CA1BX;;AAAA,wCAiCE,GANUX,sCAAZL,oBAKM,KALN,eAKM,cAHFD,oBAEM,KAFN,EAEM,IAFN,EAEM,CADFA,oBAA0E,QAA1E,EAA0E;AAAlED,SAAK,EAAC,MAA4D;AAApDa,WAAK;AAAA,aAAEC,aAAQC,IAAR,CAAY,GAAZ,CAAF;AAAA;AAA+C,GAA1E,EAAgD,mBAAhD,CACE,CAFN,CAGE,CALN,sCAME,CAlCN,CAoCE,CAzCN","names":["class","_createElementVNode","_createElementBlock","_createVNode","_component_router_link","to","_hoisted_4","$options","_Fragment","key","_hoisted_7","_component_cart_list","onUpdate","onClick","_ctx","push","_hoisted_10","_toDisplayString","_hoisted_12"],"sourceRoot":"","sources":["/Users/juan/Downloads/estore/resources/js/vue/views/Cart.vue"],"sourcesContent":["<template>\n    <div class=\"cart\">\n        <ul class=\"breadcrumb\">\n            <li><router-link to=\"/\">Home</router-link></li>\n            <li>Cart</li>\n        </ul>\n        <div class=\"container\">\n            <template v-if=\"hasProducts\">\n                <div class=\"t-container\">\n                    <table>\n                        <tr>\n                            <th>Product</th>\n                            <th>Price</th>\n                            <th>Quantity</th>\n                        </tr>\n                        <cart-list @update=\"updated\" />\n                    </table>\n                </div>\n                <div class=\"total\">\n                    <div class=\"continue\">\n                        <button class=\"gray\" @click=\"$router.push('/')\">Continue shopping</button>\n                    </div>\n                    <div class=\"tlt\">\n                        Subtotal <br>\n                        <small>Shipping & taxes calculated at checkout</small>\n                    </div>\n                    <div class=\"_t\">\n                        {{total}}\n                    </div>\n                    <div class=\"chout\">\n                        <button class=\"dark\">Checkout </button>\n                    </div>\n                </div>\n            </template>\n            <div v-if=\"!hasProducts\" class=\"empty\">\n                Looks like your cart is empty\n                <div>\n                    <button class=\"gray\" @click=\"$router.push('/')\">Continue shopping</button>\n                </div>\n            </div>\n        </div>\n\n    </div>\n</template>\n\n<script>\nimport CartList from \"../components/CartList\";\nexport default {\n    name: \"Cart\",\n    components:{CartList,},\n    data(){\n        return {\n            products: [],\n        };\n    },\n    methods:{\n        updated(products){\n            this.products = products;\n        }\n    },\n    computed:{\n        total(){\n            let val = 0;\n            this.products.map(p=>val+=p.product.price * p.quantity);\n            if(isNaN(val))val = 0;\n            else val = Math.round((val + Number.EPSILON) * 100) / 100;\n            return \"$\" + val.toFixed(2);\n        },\n        hasProducts(){\n            for (const title in this.$store.state.cart) {\n                /* eslint-disable  no-prototype-builtins */\n                if (this.$store.state.cart.hasOwnProperty(title)) {\n                    return true;\n                }\n            }\n            return false;\n        }\n    }\n}\n</script>\n\n<style scoped lang=\"scss\">\n.cart{\n    display: flex;\n    flex-direction: column;\n    .t-container{\n        border-radius: 3px;\n        border: 1px solid #88929b;\n        overflow: hidden;\n    }\n    .empty{\n        flex: 1;\n        display: flex;\n        flex-direction: column;\n        text-align: center;\n        justify-content: center;\n        >div{\n            margin-top: 40px;\n        }\n    }\n    .container{\n        flex: 1;\n        display: flex;\n        flex-direction: column;\n\n    }\n    table{\n        width: 100%;\n        overflow: hidden;\n        border-collapse: collapse;\n\n        ::v-deep{\n            tr{\n                >th{\n                    background: #f7f9fa;\n                    color: #88929b;\n                }\n                >*{\n                    border-bottom: 1px solid #adb6b9;\n                    padding: 10px;\n                    &:first-child{\n                        width: 60%;\n                    }\n                    &:last-child{\n                        text-align: right !important;\n                    }\n                }\n                &:last-child{\n                    >*{\n                        border-bottom: none;\n                    }\n                }\n            }\n        }\n    }\n    button{\n        white-space: nowrap;\n    }\n    .total{\n        margin-top: 40px;\n        display: flex;\n        >div{\n            display: flex;\n            align-items: center;\n        }\n        .tlt{\n            width: 70%;\n            flex-direction: column;\n            align-items: stretch;\n            text-align: right;\n            margin-right: 30px;\n        }\n        ._t{\n            width: 30%;\n            font-weight: bold;\n        }\n    }\n}\n</style>\n"]}]}